        //   "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
        //   "type": "node"
        // },
        {
          // "background": "#6CA35E",
          "foreground": "#6CA35E",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<b>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }} {{ .Full }}</b>",
          "type": "node"
        }
